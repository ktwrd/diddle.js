{
	"recurseDepth": 10,
	"source": {
		"include": ["source-dev"],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
	},
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": false
	},
	"opts": {
		"recurse": true,
		"destination": ".",
		"template": "./node_modules/minami"
	},
	"plugins": [
		"plugins/markdown"
	]
}
