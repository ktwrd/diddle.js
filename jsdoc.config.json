{
    "recurseDepth": 10,
    "source": {
        "include": ["engine"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false
    },
    "opts": {
        "recurse": true,
        "destination": "./../diddle.js-docs/",
        "template": "./node_modules/minami"
    },
    "plugins": [
        "plugins/markdown"
    ]
}